{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.ts"
    },
    {
      "src": "/health",
      "dest": "/backend/index.ts"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "/backend/index.ts"
    }
  ]
}
